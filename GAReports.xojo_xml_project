  <ItemName>DrawFooter</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1451452415</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub DrawFooter()</SourceLine>
   <SourceLine>// footer with 2 columns, date and pagenumber</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Const left = 0</SourceLine>
   <SourceLine>Const right = 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.FT_ON  Then</SourceLine>
   <SourceLine>Var footerwidth As Double = Settings.FooterWidth</SourceLine>
   <SourceLine>Var footertable As DynaPDFTableMBS = CreateTable(1, 2, footerwidth, GRfont_small)</SourceLine>
   <SourceLine>Call footertable.AddRow()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Call footertable.SetColWidth 0, footerwidth / 2, False</SourceLine>
   <SourceLine>Call footertable.SetColWidth 1, footerwidth / 2, False</SourceLine>
   <SourceLine>Call footertable.SetFontSize - 1, -1, GRfont_small</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CellPadding footertable, AllRows, AllColumns, 1, 1, 1, 1</SourceLine>
   <SourceLine>CellBorderThin footertable, AllRows, AllColumns, "ltrb"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.FT_ODDEVEN Then</SourceLine>
   <SourceLine>If PageCount = 1 Then</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, right, DynaPDFMBS.ktaRight, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PAGESTR + PageCount.ToString</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, left, DynaPDFMBS.ktaLeft, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PRINTSTR + DateTime.Now.ToString(Locale.Current, DateTime.FormatStyles.Short) </SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If PageCount Mod 2 = 0 Then ' gerade Seitennummer</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, left, DynaPDFMBS.ktaLeft, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PAGESTR + PageCount.ToString</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, right, DynaPDFMBS.ktaRight, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PRINTSTR + DateTime.Now.ToString(Locale.Current, DateTime.FormatStyles.Short)</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, right, DynaPDFMBS.ktaRight, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PAGESTR + PageCount.ToString</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, left, DynaPDFMBS.ktaLeft, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PRINTSTR + DateTime.Now.ToString(Locale.Current, DateTime.FormatStyles.Short)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, right, DynaPDFMBS.ktaRight, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PAGESTR + PageCount.ToString</SourceLine>
   <SourceLine>Call footertable.SetCellText 0, left, DynaPDFMBS.ktaLeft, DynaPDFTableMBS.kcoCenter, _</SourceLine>
   <SourceLine>Settings.FT_PRINTSTR + DateTime.Now.ToString(Locale.Current, DateTime.FormatStyles.Short)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Call footertable.DrawTable(Settings.LeftMarginFooter, PageHeight - Settings.BottomMargin - GRfont_small)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
