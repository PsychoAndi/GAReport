  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function Init(GRFilename As String) As Boolean</SourceLine>
   <SourceLine>// inititalize report, save it on desktop</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>PDFFile = SpecialFolder.Desktop.Child(GRFilename)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If CreateNewPDF(PDFFile) Then</SourceLine>
   <SourceLine>Call SetPageCoords GReport.kpcTopDown    </SourceLine>
   <SourceLine>Call SetPageFormat GReport.kpfDIN_A4     ' = 595, 842</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>PageWidth = GetPageWidth</SourceLine>
   <SourceLine>PageHeight = GetPageHeight</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// get maxheight for datatable</SourceLine>
   <SourceLine>MaxHeight = PageHeight - Settings.TopMargin - Settings.BottomMargin - Settings.FT_Gap  </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.ReportDBFile = Nil Then</SourceLine>
   <SourceLine>Message = ("No DB!")</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.ReportSQL = "" Then</SourceLine>
   <SourceLine>Message = ("No SQL!")</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.ColumnTitles = "" Then</SourceLine>
   <SourceLine>Message = ("No columntitles!")</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.ColumnWidths = "" Then</SourceLine>
   <SourceLine>Message = ("No columnwidths!")</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.LeftMarginTitle = 0 Then</SourceLine>
   <SourceLine>Settings.LeftMarginTitle = Settings.LeftMargin</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.LeftMarginFooter = 0 Then</SourceLine>
   <SourceLine>Settings.LeftMarginFooter = Settings.LeftMargin</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var colWidths() As String = Settings.ColumnWidths.Split(",")</SourceLine>
   <SourceLine>For Each colWidth As String In colWidths</SourceLine>
   <SourceLine>Settings.TableWidth = Settings.TableWidth + colWidth.ToDouble</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>ColumnCount = colWidths.Count</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.TitleWidth = 0 Then </SourceLine>
   <SourceLine>Settings.TitleWidth = Settings.TableWidth </SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If Settings.FooterWidth = 0 Then </SourceLine>
   <SourceLine>Settings.FooterWidth = Settings.TableWidth </SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ReportDB = New SQLiteDatabase</SourceLine>
   <SourceLine>ReportDB.DatabaseFile = Settings.ReportDBFile</SourceLine>
   <SourceLine>ReportDB.Connect</SourceLine>
   <SourceLine>ReportDataRows = GetData</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return True</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Message = "Cannot create report..."</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
