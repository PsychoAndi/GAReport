   <SourceLine>Sub CellBorderThin(tbl As DynaPDFTableMBS, row As Integer, column As Integer, lefttoprightbottom As String)</SourceLine>
   <SourceLine>// Thion border</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var THINBORDER As Double = 0.15</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var left As Double</SourceLine>
   <SourceLine>Var top As Double</SourceLine>
   <SourceLine>Var right As Double</SourceLine>
   <SourceLine>Var bottom As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If lefttoprightbottom.Contains("l") Then</SourceLine>
   <SourceLine>left = THINBORDER</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If lefttoprightbottom.Contains("t") Then</SourceLine>
   <SourceLine>top = THINBORDER</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If lefttoprightbottom.Contains("r") Then</SourceLine>
   <SourceLine>right = THINBORDER</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>If lefttoprightbottom.Contains("b") Then</SourceLine>
   <SourceLine>bottom = THINBORDER</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Call tbl.SetBorderWidth row, column, left, top, right, bottom</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
