  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function DrawTitle() As Double</SourceLine>
   <SourceLine>//</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Call Append</SourceLine>
   <SourceLine>PageCount = PageCount + 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// create titletable</SourceLine>
   <SourceLine>Var rownum As Integer</SourceLine>
   <SourceLine>Var titleheight As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var TitleTable As DynaPDFTableMBS = CreateTable(1, 1, Settings.TitleWidth, GRfont_big)</SourceLine>
   <SourceLine>rownum = TitleTable.AddRow(-1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CellPadding TitleTable, rownum, 0, 1, 1, 1, 1</SourceLine>
   <SourceLine>CellBorderThick TitleTable, rownum, 0, "ltrb"</SourceLine>
   <SourceLine>CellText TitleTable, rownum, 0, Settings.ReportTitle, GRfont_big, True</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// check height</SourceLine>
   <SourceLine>If TitleTable.GetTableHeight + Settings.TopMargin + Settings.T_Gap &lt; MaxHeight Then</SourceLine>
   <SourceLine>titleheight = TitleTable.DrawTable(Settings.LeftMarginTitle, Settings.TopMargin) </SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Return -1</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// calculate new maxheight</SourceLine>
   <SourceLine>Var tableheight As Double = Settings.TopMargin + titleheight + Settings.T_Gap</SourceLine>
   <SourceLine>MaxHeightData = MaxHeight - tableheight</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return Settings.TopMargin + titleheight + Settings.T_Gap</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
