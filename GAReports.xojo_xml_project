 <Method>
  <ItemName>MakeReport</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1676967935</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub MakeReport(GReportFilename As String, ShowIt As Boolean, Optional Viewer As DesktopHTMLViewer)</SourceLine>
   <SourceLine>// create report</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Init(GReportFilename) Then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var yPos As Double = DrawTitle()</SourceLine>
   <SourceLine>Var datatable As DynaPDFTableMBS = DataTableCreate()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For Each datarow As String In ReportDataRows</SourceLine>
   <SourceLine>If Not DataTableAddRow(datatable, datarow) Then</SourceLine>
   <SourceLine>// actual page, optional footer</SourceLine>
   <SourceLine>Call datatable.DrawTable(Settings.LeftMargin, yPos)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Settings.FT_ON Then</SourceLine>
   <SourceLine>DrawFooter()</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Call EndPage</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// new page </SourceLine>
   <SourceLine>yPos = DrawTitle()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// with new datatable</SourceLine>
   <SourceLine>datatable = DataTableCreate()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add the row, that did not fit on the old page</SourceLine>
   <SourceLine>Call DataTableAddRow(datatable, datarow)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// end of report, draw last (or the only) page</SourceLine>
   <SourceLine>Call datatable.DrawTable(Settings.LeftMargin, yPos)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// optional footer</SourceLine>
   <SourceLine>If Settings.FT_ON  Then</SourceLine>
   <SourceLine>DrawFooter()</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Call EndPage</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If CloseFile Then</SourceLine>
   <SourceLine>// if there is a htmlviewer, show it there, otherwise show external if ShowIt=true</SourceLine>
   <SourceLine>If Viewer = Nil Then</SourceLine>
   <SourceLine>If ShowIt Then</SourceLine>
   <SourceLine>PDFFile.Open</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Viewer.LoadPage(PDFFile)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>MessageBox("Error: " + Message)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>GReportFilename As String, ShowIt As Boolean, Optional Viewer As DesktopHTMLViewer</ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>DataTableAddRow</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1475092479</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function DataTableAddRow(tbl As DynaPDFTableMBS, row As String) As Boolean</SourceLine>
   <SourceLine>// add row to tbl if row fits on page</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If tbl.GetTableHeight &lt; MaxHeightData Then</SourceLine>
   <SourceLine>Var rownum As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>rownum = tbl.AddRow(-1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var celltexts() As String = row.Split(";")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// write text with border</SourceLine>
   <SourceLine>For i As Integer = 0 To celltexts.Count - 1</SourceLine>
   <SourceLine>If i = 0 Then</SourceLine>
   <SourceLine>CellBorderThin tbl, rownum, i, "ltrb"</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>CellBorderThin tbl, rownum, i, "trb"</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CellPadding tbl, rownum, AllColumns, 1, 1, 1, 1</SourceLine>
   <SourceLine>CellText tbl, rownum, i, celltexts(i), GRfont_normal</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return True</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>// row does not fit on page</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>tbl As DynaPDFTableMBS, row As String</ItemParams>
  <ItemResult>Boolean</ItemResult>
 </Method>
 <Method>
  <ItemName>DataTableCreate</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>985835519</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function DataTableCreate() As DynaPDFTableMBS</SourceLine>
   <SourceLine>// creates a table with one row containing columntitles</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var tbl As DynaPDFTableMBS = CreateTable(1, ColumnCount, Settings.TableWidth, GRfont_normal)</SourceLine>
   <SourceLine>Call tbl.AddRow(-1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// creates arrays of titles and widths</SourceLine>
   <SourceLine>Var ColumnTitles() As String = Settings.ColumnTitles.Split(",")</SourceLine>
   <SourceLine>Var ColumnWidths() As String = Settings.ColumnWidths.Split(",")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For i As Integer = 0 To ColumnTitles.Count - 1</SourceLine>
   <SourceLine>Call tbl.SetColWidth i, ColumnWidths(i).ToDouble, False</SourceLine>
   <SourceLine>If i = 0 Then</SourceLine>
   <SourceLine>CellBorderThin tbl, 0, i, "ltrb"</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>CellBorderThin tbl, 0, i, "trb"</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Call tbl.SetCellPadding 0, AllColumns, 1, 1, 1, 1</SourceLine>
   <SourceLine>CellText tbl, 0, i, ColumnTitles(i), GRfont_normal, True</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return tbl</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult>DynaPDFTableMBS</ItemResult>
 </Method>
