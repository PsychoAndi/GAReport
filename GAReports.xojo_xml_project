 <Method>
  <ItemName>GetData</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>97875967</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function GetData() As String()</SourceLine>
   <SourceLine>// gets data from db</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var rowsFound As RowSet</SourceLine>
   <SourceLine>Var dbcolumns() As String</SourceLine>
   <SourceLine>Var dbrows() As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>rowsFound = ReportDB.SelectSQL(Settings.ReportSQL)</SourceLine>
   <SourceLine>If rowsFound &lt;&gt; Nil Then</SourceLine>
   <SourceLine>For Each dbrow As DatabaseRow In rowsFound</SourceLine>
   <SourceLine>dbcolumns.RemoveAll</SourceLine>
   <SourceLine>For i As Integer = 0 To rowsFound.ColumnCount - 1</SourceLine>
   <SourceLine>// maybe a SQLDate in one of the columns, convert it to a readable format</SourceLine>
   <SourceLine>If i = Settings.DateColumn Then</SourceLine>
   <SourceLine>Var dt As DateTime = DateTime.FromString(dbrow.ColumnAt(i).StringValue)</SourceLine>
   <SourceLine>dbcolumns.Add(dt.ToString(Locale.Current, DateTime.FormatStyles.Medium, DateTime.FormatStyles.None))</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>dbcolumns.Add(dbrow.ColumnAt(i).StringValue)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>dbrows.Add(String.FromArray(dbcolumns, ";")) </SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>rowsFound.Close</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Catch Error As DatabaseException</SourceLine>
   <SourceLine>MessageBox("Database error...")</SourceLine>
   <SourceLine>Exit Function</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ReportDB.Close</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// for debugging</SourceLine>
   <SourceLine>For i As Integer = 0 To dbrows.LastIndex</SourceLine>
   <SourceLine>System.DebugLog(dbrows(i))</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>System.DebugLog("Rows: " + Cstr(dbrows.Count))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return dbrows()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult>String()</ItemResult>
