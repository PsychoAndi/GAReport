   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub CellText(tbl As DynaPDFTableMBS, row As Integer, column As Integer, ctext As String, fontsize As Double, Optional bold As Boolean, Optional align As Integer)</SourceLine>
   <SourceLine>// writes text into cell</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If bold Then</SourceLine>
   <SourceLine>Call tbl.SetFont row, column, "Helvetica", DynaPDFMBS.kfsBold, True, DynaPDFMBS.kcpUnicode</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Standard ist Linksbündig</SourceLine>
   <SourceLine>If align = 0 Then</SourceLine>
   <SourceLine>align = DynaPDFMBS.ktaLeft</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Call tbl.SetFontSize row, column, fontsize</SourceLine>
   <SourceLine>Call tbl.SetCellText row, column, align, DynaPDFTableMBS.kcoCenter, ctext</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
